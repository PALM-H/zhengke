/**mine.wxss**/
.page{
  overflow: hidden;
}
.topimg{
  width: 100%;
  height: 171rpx;
}
.headimg{
  position: absolute;
  top: 30rpx;
  width: 100%;
  text-align: center;
}
.headimg image{
  width: 200rpx;
  height: 200rpx;
  border-radius: 50%;
}
.nickName{
  margin-top: 86rpx;
  width: 100%;
  font-size: 40rpx;
  font-weight: bold;
  color: #4a4a4a;
  text-align: center;
}
.menuwrap{
  position: relative;
  margin-top: 50rpx;
  width: 100%;
}
.menuwrap .middlewrap{
  border-top: 2rpx solid #f6f6f6;
  border-bottom: 2rpx solid #f6f6f6;
}
.menuwrap .item{
  position: relative;
  display: block;
  height: 216rpx;
  -webkit-box-flex: 1;
  -webkit-flex: 1;
  -ms-flex: 1;
  flex: 1;
  text-align: center;
  -webkit-tap-highlight-color: transparent;
}
.menuwrap .item.rightitem{
  border-left: 2rpx solid #f6f6f6;
}
.menuwrap .item .itemimg{
  margin-top: 50rpx;
  width: 100%;
  height: 80rpx;
  text-align: center;
}
.menuwrap .item .itemimg image{
  width: 80rpx;
  height: 80rpx;
}
.menuwrap .item .itemtext{
  margin-top: 8rpx;
  width: 100%;
  font-size: 34rpx;
  font-weight: bold;
  color: #4a4a4a;
  text-align: center;
}
/**Invitecode.wxss**/
.invitewrap{
	position: fixed;
	width: 100%;
	top: 50%;
	transform: translateY(-50%);
	text-align: center;
}
.signimg{
	width: 150rpx;
	height: 111rpx;
}
.titletext{
	display: block;
	margin-top: 46rpx;
	font-size: 48rpx;
	font-weight: bold;
	color: #4a4a4a;
}
.invitecode{
	display: block;
	width: 100%;
	height: 146rpx;
	line-height: 146rpx;
	margin-top: 50rpx;
	border-top: 1rpx solid #eeeeee;
	border-bottom: 1rpx solid #eeeeee;
	font-size: 48rpx;
	font-weight: bold;
	color: #0080ff;
}
.leadtext{
	display: block;
	margin-top: 40rpx;
	font-size: 34rpx;
	color: #4a4a4a;
}
.tipstext{
	display: block;
	margin-top: 20rpx;
	font-size: 20rpx;
	color: #9b9b9b;
}